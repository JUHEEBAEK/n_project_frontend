{
  "name": "n_project_frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --mode development",
    "serve:prod": "vue-cli-service serve --mode production",
    "build:dev": "vue-cli-service build --mode development --dist",
    "build:prod": "vue-cli-service build --mode production --dist",
    "prebuild": "rm -rf dist",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit",
    "start": "vue-cli-service serve",
    "nohup": "nohup vue-cli-service serve --mode production &"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.8.2",
    "axios": "^0.19.2",
    "core-js": "^3.4.4",
    "d3": "^5.16.0",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.19",
    "moment": "^2.24.0",
    "register-service-worker": "^1.6.2",
    "roboto-fontface": "*",
    "vue": "^2.6.11",
    "vue-cli-plugin-s3-deploy": "^4.0.0-rc3",
    "vue-cookies": "^1.1.4",
    "vue-router": "^3.1.3",
    "vue-server-renderer": "^2.6.11",
    "vuetify": "^2.1.0",
    "vuex": "^3.1.2",
    "vuex-mock-store": "0.0.8"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.1.0",
    "@vue/cli-plugin-eslint": "^4.1.0",
    "@vue/cli-plugin-pwa": "^4.1.0",
    "@vue/cli-plugin-unit-jest": "^4.1.0",
    "@vue/cli-service": "^4.4.4",
    "@vue/eslint-config-prettier": "^5.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-eslint": "^10.0.3",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^5.0.0",
    "jest-serializer-vue": "^2.0.2",
    "node-sass": "^4.14.1",
    "prettier": "^1.19.1",
    "sass": "^1.19.0",
    "sass-loader": "^8.0.0",
    "vue-cli-plugin-vuetify": "^2.0.3",
    "vue-jest": "^3.0.5",
    "vue-template-compiler": "^2.6.10",
    "vuetify-loader": "^1.4.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/recommended",
      "prettier/vue"
    ],
    "rules": {
      "no-console": "off",
      "no-unused-vars": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "jest": {
    "preset": "@vue/cli-plugin-unit-jest",
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "vue-jest"
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-vue"
    ]
  }
}
